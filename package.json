{
  "name": "mat3-webcomponent",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run webpack",
    "clean-build-dir": "rm -rf dist/mat3-webcomponent/browser dist/mat3-webcomponent/3rdpartylicenses.txt",
    "webpack": "webpack && npm run clean-build-dir && npm run webcomp-test",
